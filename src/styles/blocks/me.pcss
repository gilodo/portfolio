.me {
    background: url('~images/bg/me.jpg') center center / cover no-repeat;
    position: relative;
    &:before {
        content: '';
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
       display: block; 
        background: linear-gradient(-90deg,transparent,rgba(28,32,36,.7) 40%);
    }
}